<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Cognizant">
	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener
			class-name="org.uncommons.reportng.JUnitXMLReporter" />
		<listener class-name="com.cognizant.listeners.CustomListeners" />
	</listeners>
	
	<test thread-count="5" name="SimpleLinksTest">
		<classes>
			<class name="com.cognizant.testcases.SimpleLinksTest" />
		</classes>
	</test> <!-- Test -->
	<test thread-count="5" name="NoJSLinksTest">
		<classes>
			<class name="com.cognizant.testcases.NoJSLinksTest" />
		</classes>
	</test> <!-- Test -->
	
</suite> <!-- Suite -->
